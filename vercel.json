{
    "builds": [{
        "src": "expense_tracker/expense_tracker/expense_tracker/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb", "runtime": "python3.9" }
    }],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "expense_tracker/expense_tracker/expense_tracker/wsgi.py"
        }
    ],
    "build":{
        "env":{
            "PYTHONPATH":"expense_tracker/expense_tracker"
        }
    }
}