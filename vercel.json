{
    "builds": [
        {
        "src": "expense_tracker/wsgi.py",
        "use": "@vercel/python",
        "config": { "maxLambdaSize": "50mb", "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "./expense_tracker_app/static/(.*)",
            "dest": "./expense_tracker_app/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "expense_tracker/wsgi.py"
        }
    ]
    
}